<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ycl.tabview">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <application
        android:name=".application.Myapplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".Activity.MainActivity"></activity>
        <activity android:name=".Activity.RegisterActivity"></activity>
        <activity android:name=".Activity.SearchActivity"></activity>
        <activity android:name=".Activity.SettingActivity"></activity>
        <activity android:name=".Activity.UpdateUserActivity"></activity>
        <activity android:name=".Activity.UserInfoActivity"></activity>
        <activity android:name=".Activity.ChooseActivity"></activity>
        <activity android:name=".Activity.GoodsActivity"></activity>
        <activity android:name=".Activity.MyGoodsActivity"></activity>
        <activity android:name=".Activity.AddgoodsActivity"></activity>
        <activity android:name=".Activity.AddSignActivity"></activity>
        <activity android:name=".Activity.TakeGoodsActivity"></activity>
        <activity android:name=".Activity.LoseGoodsActivity"></activity>
        <activity android:name=".Activity.AddMessageActivity"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>